<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".character">

    <TextView
        android:id="@+id/str"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/STR"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <TextView
        android:id="@+id/con"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/CON"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/str" />

    <TextView
        android:id="@+id/inte"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/INT"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/con" />

    <TextView
        android:id="@+id/dex"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/DEX"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inte" />

    <TextView
        android:id="@+id/cha"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="56dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/CHA"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dex" />

    <TextView
        android:id="@+id/sstr"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/job" />

    <TextView
        android:id="@+id/ccon"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sstr" />

    <TextView
        android:id="@+id/iint"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ccon" />

    <TextView
        android:id="@+id/ddex"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iint" />

    <TextView
        android:id="@+id/ccha"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ddex" />

    <TextView
        android:id="@+id/name"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/rua" />

    <TextView
        android:id="@+id/race"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:text="@string/rua"
        android:textSize="15sp"
        app:layout_constraintEnd_toStartOf="@+id/job"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toEndOf="@+id/name"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/rua" />

    <TextView
        android:id="@+id/job"
        android:layout_width="100dp"
        android:layout_height="36dp"
        android:layout_marginEnd="32dp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="32dp"
        android:gravity="center"
        android:textSize="15sp"
        android:text="@string/rua"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/rua" />

</android.support.constraint.ConstraintLayout>